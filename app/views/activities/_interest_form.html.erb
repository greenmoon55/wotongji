<li id="interested-users-num" style="display: block; margin-bottom: 10px">
  <%= @activity.interestedusers.count %>
  <b> 人感兴趣</b>
</li>
<li id="interest-form">
    <% if current_user.interested_in?(@activity) %>
      <%= render 'uninterest' %>
    <% else %>
      <%= render 'interest' %>
    <% end %>
</li>
